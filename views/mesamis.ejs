<body>
    <h1>pIcTuReMe</h1>
    <h3>Mes amis</h3>

    <div class="container">
        <div id="users">

            <h2 class="form-signin-heading">Qui voulez-vous retrouver ?</h2>
            <label for="inputAmis" class="sr-only">Nom/Prenom/Adresse mail</label>
            <input type="text" id="inputAmis" class="search" placeholder=" Nom/Prenom/Adresse mail " required autofocus name="amis">




            <button class="sort" data-sort="name">
    Filtrer par nom
  </button>
            <button class="sort" data-sort="pseudo">
    Filtrer par pseudo
  </button>

            <ul class="list">
                <li>
                    <h5 class="name">Seycha Senth</h5>
                    <p class="pseudo">sth.01 </p>
                </li>
                <li>
                    <h5 class="name">Jeyam Celya</h5>
                    <p class="pseudo">Celya13 </p>
                </li>
            </ul>



        </div>
        <div>
        
            <% for (var i=0 ; i < friends.length; i++){ %>
                <p> <% typeof friends[i].id_user != 'undefined' ? friends[i].id_user : '' %> </p>
                <p> <% typeof friends[i].name != 'undefined' ? friends[i].name : '' %>  </p>

            <% } %>
            <% JSON.stringify(friends) %>
        </div>

        <a href="/accueil">Retourner sur le tableau de bord</a>

</body>

</html>